{
  "name": "typemold",
  "version": "1.0.0",
  "description": "A lightweight, high-performance object mapper for TypeScript and Node.js with runtime field projection",
  "author": {
    "name": "Chetan Joshi",
    "email": "chetanjoshi707@gmail.com",
    "url": "https://github.com/ErrorX407"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "benchmark": "ts-node benchmarks/index.ts",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "mapper",
    "object-mapper",
    "dto",
    "typescript",
    "nestjs",
    "nodejs",
    "transform",
    "serialization",
    "automapper",
    "type-safe"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ErrorX407/typemold.git"
  },
  "bugs": {
    "url": "https://github.com/ErrorX407/typemold/issues"
  },
  "homepage": "https://github.com/ErrorX407/typemold#readme",
  "peerDependencies": {
    "reflect-metadata": "^0.1.13 || ^0.2.0",
    "@nestjs/common": ">=9.0.0",
    "@nestjs/core": ">=9.0.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/common": {
      "optional": true
    },
    "@nestjs/core": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "class-validator": ">=0.14.0"
  },
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "class-validator": "^0.14.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "reflect-metadata": "^0.2.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
